n\n```ts\nimport { trpc } from '~/utils/trpc';\n\nexport function Hello() {\n  const { data, error, status } = trpc.greeting.useQuery({ name: 'tRPC' });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/react-query@10.7.1-alpha-HEAD-2023-09-28-18-58-14.81","_nodeVersion":"18.18.0","_npmVersion":"lerna/7.0.0/node@v18.18.0+x64 (linux)","dist":{"integrity":"sha512-q3cr+4ZAFjKOQpDlcugS9JgPwNS5ZrhTcasc3KpsXFnr9Xb9bUbA5wmZWdtFDk7jVVHaaAZ/1gPNaGWljMx9LA==","shasum":"b685b28f4efcff548c4e2215570d5d34b34a2d54","tarball":"https://registry.npmjs.org/@trpc/react-query/-/react-query-10.7.1-alpha-HEAD-2023-09-28-18-58-14.81.tgz","fileCount":95,"unpackedSize":248082,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEQCIBalRcmeuI/mTt0peOZ9eyD8BADHivjHnTeAbXLObz5GAiBE/1AtcX8FSh3NeOVErEfmXP6MGGm/nTbie48F1PgSaQ=="}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/react-query_10.7.1-alpha-HEAD-2023-09-28-18-58-14.81_1695927671961_0.9572170039638475"},"_hasShrinkwrap":false},"10.7.1-alpha-HEAD-2023-09-28-23-45-54.81":{"name":"@trpc/react-query","version":"10.7.1-alpha-HEAD-2023-09-28-23-45-54.81","description":"The tRPC React library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/react"},"scripts":{"build":"rollup --config rollup.config.ts --configPlugin roll