ery({ name: 'tRPC' });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/react-query@10.7.1-alpha-next-2023-09-27-02-47-10.74","_nodeVersion":"18.17.1","_npmVersion":"lerna/7.0.0/node@v18.17.1+arm64 (darwin)","dist":{"integrity":"sha512-ycT6MaNVJjL6hWyej6uD/Oc9cZHEUf8njLTjFc2Jjou4h0dCglL6nDE8L22CzrDVTZcTFo40UAnZT9eE/x9DsQ==","shasum":"d338e577b538b965111cd1c3c682410f6b7f645c","tarball":"https://registry.npmjs.org/@trpc/react-query/-/react-query-10.7.1-alpha-next-2023-09-27-02-47-10.74.tgz","fileCount":94,"unpackedSize":236192,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEUCIC9Rf5JS+6VBSzEILS5etxW9ZDeL0TZku2Huxq0JiNJeAiEAhfjOOF+JTI06UAEf2IofzwZjVb17HjmNbRb8qNRcEro="}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/react-query_10.7.1-alpha-next-2023-09-27-02-47-10.74_1695802410559_0.7888095644813706"},"_hasShrinkwrap":false},"10.7.1-alpha-next-2023-09-27-10-25-04.76":{"name":"@trpc/react-query","version":"10.7.1-alpha-next-2023-09-27-10-25-04.76","description":"The tRPC React library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/react"},"scripts":{"build":"rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3","dev":"pnpm build --watch","codegen-entrypoints":"tsx entrypoints.script.ts","lint":"eslint --cache --ext \".js