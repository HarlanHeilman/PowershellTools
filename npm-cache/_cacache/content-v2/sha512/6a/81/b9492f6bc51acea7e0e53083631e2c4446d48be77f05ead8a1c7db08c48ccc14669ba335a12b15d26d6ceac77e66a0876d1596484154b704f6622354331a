 'tRPC' });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/react-query@11.0.0-alpha-tmp-01-26-no-proc-router-rec.260","_nodeVersion":"20.11.0","_npmVersion":"lerna/8.0.0/node@v20.11.0+x64 (linux)","dist":{"integrity":"sha512-0XN/eibJPevGTSc7S3sczDrkuACO74bDKs234nYKHdOKcUX2CC26as2HR39H9IPNgzEa4siHcKdmxrRcA9pNSg==","shasum":"a08beb34f2bb38525b4c133ae34bcf4d843891c8","tarball":"https://registry.npmjs.org/@trpc/react-query/-/react-query-11.0.0-alpha-tmp-01-26-no-proc-router-rec.260.tgz","fileCount":118,"unpackedSize":252447,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEQCIGz9xdXlTUt2lQsSNo09IAk6nuAITCElw5fNr5eYDWuKAiA+ItRmmuXLiygY7ESL/1UeaPBW2ylvZB8JCo0jTlP1kQ=="}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/react-query_11.0.0-alpha-tmp-01-26-no-proc-router-rec.260_1706320108904_0.46403342926264934"},"_hasShrinkwrap":false},"11.0.0-alpha-tmp-01-26-no-proc-router-rec.264":{"name":"@trpc/react-query","version":"11.0.0-alpha-tmp-01-26-no-proc-router-rec.264","description":"The tRPC React library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/react"},"scripts":{"build":"rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3","dev":"pnpm build --watch","codegen-entrypoints":"tsx entrypoints.script.ts","lint":"eslint --cach