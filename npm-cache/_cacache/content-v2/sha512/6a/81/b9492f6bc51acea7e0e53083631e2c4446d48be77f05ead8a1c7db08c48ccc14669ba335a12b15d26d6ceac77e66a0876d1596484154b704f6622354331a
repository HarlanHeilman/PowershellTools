`ts\nimport { trpc } from '~/utils/trpc';\n\nexport function Hello() {\n  const { data, error, status } = trpc.greeting.useQuery({ name: 'tRPC' });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/react-query@11.0.0-alpha-tmp-01-26-simplify-router.282","_nodeVersion":"20.11.0","_npmVersion":"lerna/8.0.0/node@v20.11.0+x64 (linux)","dist":{"integrity":"sha512-+CLvSO6jQfAI9+k8URlW3N7eQ1rkBulR0R5vr36OcNto2Jf/pbK3/pcV1FbAxhuBrYnnKopdqyoHPYIhigSBXg==","shasum":"f14755bfb8f07d99b92e7515bf8cc3be122be23e","tarball":"https://registry.npmjs.org/@trpc/react-query/-/react-query-11.0.0-alpha-tmp-01-26-simplify-router.282.tgz","fileCount":118,"unpackedSize":251940,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEQCIC0uSZmjayRyLy8Ial+6oyVRdMKFX3udweWed4TRqLRnAiAVa2RW1t3Lg1qVhdEUSv0Bu9wh38RYC8aS2rmsLRdSsA=="}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/react-query_11.0.0-alpha-tmp-01-26-simplify-router.282_1706279585812_0.18773601334340517"},"_hasShrinkwrap":false},"11.0.0-alpha-tmp-01-26-simplify-router.283":{"name":"@trpc/react-query","version":"11.0.0-alpha-tmp-01-26-simplify-router.283","description":"The tRPC React library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/react"},"scripts":{"build":"rollup --config rollup.config.ts --configPlug