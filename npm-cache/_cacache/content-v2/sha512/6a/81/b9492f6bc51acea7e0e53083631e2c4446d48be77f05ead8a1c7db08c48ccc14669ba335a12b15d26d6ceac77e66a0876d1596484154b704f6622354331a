 trpc } from '~/utils/trpc';\n\nexport function Hello() {\n  const { data, error, status } = trpc.greeting.useQuery({ name: 'tRPC' });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/react-query@11.0.0-alpha-tmp-01-26-no-proc-router-rec.269","_nodeVersion":"20.11.0","_npmVersion":"lerna/8.0.0/node@v20.11.0+x64 (linux)","dist":{"integrity":"sha512-deaHQ+Gh3IWPuAyKubrl4Fq5O1+BZ86EpHSQVfxLm1gkOOsb25zLCLJ+bpbx/Cp0HeARenoN1zb462DHVt8THQ==","shasum":"48df3b00fe453a2dd6ee56c3c49d05325cd19140","tarball":"https://registry.npmjs.org/@trpc/react-query/-/react-query-11.0.0-alpha-tmp-01-26-no-proc-router-rec.269.tgz","fileCount":118,"unpackedSize":252339,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEYCIQDAtWFLc//SWeZyOueLST6mcBI+wl1jimNvCZsSwL9H8QIhALGoJH+eY0705Fd5fyUvOK9QFLrKAyLlbIfztgFgTt5Q"}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/react-query_11.0.0-alpha-tmp-01-26-no-proc-router-rec.269_1706451836377_0.03921035805968809"},"_hasShrinkwrap":false},"11.0.0-alpha-tmp-01-26-no-proc-router-rec.271":{"name":"@trpc/react-query","version":"11.0.0-alpha-tmp-01-26-no-proc-router-rec.271","description":"The tRPC React library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/react"},"scripts":{"build":"rollup --config rollup.config.ts --configPl