greeting.useQuery({\n    name: 'tRPC',\n  });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/next@10.7.1-alpha-HEAD-2023-09-29-13-56-35.81","_nodeVersion":"18.18.0","_npmVersion":"lerna/7.0.0/node@v18.18.0+x64 (linux)","dist":{"integrity":"sha512-rUNtCFktiSytK3b2YAvDNPdSCS4OVqhmIs6DRpaivgWMLmMLmcngRe/w6bckEWEsK4BvagibdxuEU0CK0x4hxw==","shasum":"f9e8a73dae75f0826131c7c324cd062437c58a7d","tarball":"https://registry.npmjs.org/@trpc/next/-/next-10.7.1-alpha-HEAD-2023-09-29-13-56-35.81.tgz","fileCount":58,"unpackedSize":109421,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEUCIQDq23nhkVzHwh7CGEsyLuX2kmIDeyIJpBwN9yiqh1VGeQIgR3R0zKPPnzj8Lz0n3dIXq6GPPHMJV16ikqd5xyso9qM="}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/next_10.7.1-alpha-HEAD-2023-09-29-13-56-35.81_1695995975408_0.8925468376401597"},"_hasShrinkwrap":false},"10.7.1-alpha-HEAD-2023-09-29-14-07-29.81":{"name":"@trpc/next","version":"10.7.1-alpha-HEAD-2023-09-29-14-07-29.81","description":"The tRPC Next.js library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/next"},"eslintConfig":{"rules":{"react-hooks/exhaustive-deps":"error","no-restricted-imports":["error","@trpc/next"]}},"scripts":{"build":"rollup --config rollup.config.ts --configPlugin rollup-plugin-swc3","dev":"pnpm b