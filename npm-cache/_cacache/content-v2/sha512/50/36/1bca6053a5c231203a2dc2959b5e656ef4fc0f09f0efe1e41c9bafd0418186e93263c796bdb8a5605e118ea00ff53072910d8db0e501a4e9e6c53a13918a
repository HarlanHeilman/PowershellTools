 App = ({ Component, pageProps }) => {\n  return <Component {...pageProps} />;\n};\n\nexport default trpc.withTRPC(App);\n```\n\nNow you can query your API in any component.\n\n```ts\nimport { trpc } from '~/utils/trpc';\n\nexport function Hello() {\n  const { data, error, status } = trpc.greeting.useQuery({\n    name: 'tRPC',\n  });\n\n  if (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/next@11.0.0-alpha-tmp.233","_nodeVersion":"20.10.0","_npmVersion":"lerna/8.0.0/node@v20.10.0+x64 (linux)","dist":{"integrity":"sha512-zgx4/TnYzN05hN6/RVa1O3gu8pS9YAlkI7tHESUsGFWSnDR8/PBi9O1itOm6BmJau7tOzB/2G51yFweGqcr4uA==","shasum":"ce4da3ffba3ed4861b1275b234e28eefa505a6aa","tarball":"https://registry.npmjs.org/@trpc/next/-/next-11.0.0-alpha-tmp.233.tgz","fileCount":58,"unpackedSize":113946,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEQCIH/tTN8ruZ0maaDrxtq9MnnDNRuUj5AqoAC0yFSPnfBeAiA9q8HShJAEqlynW7AKENAYYyPgcywJxsRbhB3CHkEcQA=="}]},"_npmUser":{"name":"katt","email":"alexander@n1s.se"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/next_11.0.0-alpha-tmp.233_1704643915105_0.36795339790638604"},"_hasShrinkwrap":false},"11.0.0-alpha-tmp.234":{"name":"@trpc/next","version":"11.0.0-alpha-tmp.234","description":"The tRPC Next.js library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/next"},"eslintConfig":{"rules