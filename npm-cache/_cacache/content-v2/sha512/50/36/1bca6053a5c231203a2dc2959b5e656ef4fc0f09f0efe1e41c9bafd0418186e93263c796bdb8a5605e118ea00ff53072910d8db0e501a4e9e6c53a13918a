f (error) {\n    return <p>{error.message}</p>;\n  }\n\n  if (status !== 'success') {\n    return <p>Loading...</p>;\n  }\n\n  return <div>{data && <p>{data.greeting}</p>}</div>;\n}\n```\n","readmeFilename":"README.md","bugs":{"url":"https://github.com/trpc/trpc/issues"},"_id":"@trpc/next@10.7.1-alpha-next-2023-08-07-23-17-43.62","_nodeVersion":"18.16.0","_npmVersion":"lerna/7.0.0/node@v18.16.0+arm64 (darwin)","dist":{"integrity":"sha512-+YAbJvTxApHpgGGLuIH6ll44jc4GCdncyWtHTiJ9kND2BWwuPNdtOHleayf5raAmtM1GTHbmvusKY2OwsQsRqQ==","shasum":"28c68805d2ff404b941dcb3e76bba27314157018","tarball":"https://registry.npmjs.org/@trpc/next/-/next-10.7.1-alpha-next-2023-08-07-23-17-43.62.tgz","fileCount":65,"unpackedSize":106639,"signatures":[{"keyid":"SHA256:jl3bwswu80PjjokCgh0o2w5c2U4LhQAE57gj9cz1kzA","sig":"MEQCIGHZjie0mSD1dubcYRI1zVP9IwL1e+RZSPGYZLZtQSv/AiBkqf8HmAi/UEGUFdd1hTWTreJRYUyZPYpT1pElqmuRZA=="}]},"_npmUser":{"name":"juliusmarminge","email":"julius0216@outlook.com"},"directories":{},"maintainers":[{"name":"juliusmarminge","email":"julius0216@outlook.com"},{"name":"sachinraja","email":"sachinraja2349@gmail.com"},{"name":"trpc-bot","email":"alexander+trpc@n1s.se"},{"name":"katt","email":"alexander@n1s.se"}],"_npmOperationalInternal":{"host":"s3://npm-registry-packages","tmp":"tmp/next_10.7.1-alpha-next-2023-08-07-23-17-43.62_1691443306107_0.2682167800167663"},"_hasShrinkwrap":false},"10.32.1-alpha-actions-2-2023-08-09-10-23-47.45":{"name":"@trpc/next","version":"10.32.1-alpha-actions-2-2023-08-09-10-23-47.45","description":"The tRPC Next.js library","author":{"name":"KATT"},"license":"MIT","main":"dist/index.js","module":"dist/index.mjs","typings":"dist/index.d.ts","homepage":"https://trpc.io","repository":{"type":"git","url":"git+https://github.com/trpc/trpc.git","directory":"packages/next"},"eslintConfig":{"rules":{"react-hooks/exhaustive-deps":"error","no-restricted-imports":["error","@trpc/next"]}},"scripts":{"build":"rollup --config rollup.config.ts --configPlugin ro